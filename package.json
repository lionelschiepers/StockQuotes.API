{
  "name": "stockquote-api",
  "version": "1.0.0",
  "main": "dist/src/functions/*.js",
  "description": "",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:production": "npm run prestart:production && npm prune --production",

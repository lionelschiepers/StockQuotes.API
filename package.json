  "dependencies": {
    "@azure/functions": "^4.0.0",
    "axios": "^1.13.2",
    "yahoo-finance2": "^3.10.2"
  },
  "devDependencies": {
    "azure-functions-core-tools": "^4.6.0",
    "@types/node": "25.x",
    "typescript": "^5.6.3",
    "rimraf": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2"
  }
}
